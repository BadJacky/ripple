{
  "name": "cloudtay/ripple",
  "version": "v0.8",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Ripple\\": "src/",
      "Co\\": "src/Supports/"
    },
    "files": [
      "src/functions.php",
      "src/functions-root.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "cclilshy",
      "email": "jingnigg@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-sockets": "*",
    "ext-openssl": "*",
    "psr/http-message": "*",
    "revolt/event-loop": "^1.0",
    "cloudtay/ripple-http": "*",
    "cloudtay/ripple-websocket": "*",
    "symfony/http-foundation": "*",
    "symfony/dependency-injection": "*",
    "guzzlehttp/guzzle": "*"
  },
  "require-dev": {
    "ext-posix": "*",
    "ext-pcntl": "*",
    "ext-curl": "*",
    "ext-parallel": "*",
    "ext-zlib": "*",
    "phpunit/phpunit": "*",
    "friendsofphp/php-cs-fixer": "*",
    "cloudtay/ripple-http": "dev-main",
    "cloudtay/ripple-websocket": "dev-main"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
